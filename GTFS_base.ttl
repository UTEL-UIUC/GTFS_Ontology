gtfs:is_operator_1 a gtfs:IsOperatorEnum ;
    rdfs:label "is_operator:1" ;
    gtfs:hasEnumValue "1" ;
    rdfs:comment "True" .

gtfs:is_producer_0 a gtfs:IsProducerEnum ;
